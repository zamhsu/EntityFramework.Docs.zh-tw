{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**.png",
          "**.jpg"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/ef/toc.json",
      "searchScope": [
        "Entity Framework"
      ],
      "ms.topic": "conceptual",
      "audience": "developer",
      "ms.prod": "entity-framework",
      "ms.tgt_pltfrm": "dotnet",
      "ms.devlang": "dotnet",
      "ms.author": "avickers",
      "disable_contribution_on_translation_quality": ["MT"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "aspnet/EntityFramework.Docs.zh-tw",
      "feedback_product_url": "https://github.com/aspnet/EntityFrameworkCore/issues/new",
      "uhfHeaderId": "MSDocsHeader-DotNet"
    },
    "fileMetadata": {
      "feedback_product_url": {
        "ef6/**/**.md": "https://github.com/aspnet/EntityFramework6/issues/new"
      },
      "ms.technology": {
        "core/**/**.md": "entity-framework-core",
        "ef6/**/**.md": "entity-framework-6",
        "efcore-and-ef6/**/**.md": "entity-framework-core"
      },
      "open_to_public_contributors": {
	      "core/managing-schemas/**.md": false,
        "core/miscellaneous/**.md": false,
        "core/modeling/**.md": false,
        "core/providers/**.md": false,
        "ef6/fundamentals/**.md": false,
        "ef6/modeling/**.md": false,
        "ef6/querying/**.md": false,
        "ef6/resources/**.md": false,
        "ef6/saving/**.md": false,
        "ef6/what-is-new/**.md": false,
        "efcore-and-ef6/porting/**.md": false
      }
    },
    "dest": "_site",
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig"
  }
}
