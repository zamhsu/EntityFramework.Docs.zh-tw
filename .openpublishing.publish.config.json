{
  "build_entry_point": "docs",
  "docsets_to_publish": [
    {
      "docset_name": "ef",
      "build_source_folder": "entity-framework",
      "build_output_subfolder": "ef",
      "locale": "zh-TW",
      "open_to_public_contributors": true,
      "type_mapping": {
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content"
      },
      "template_folder": "_themes"
    }
  ],
  "notification_subscribers": [
    "divega@microsoft.com"
  ],
  "branches_to_filter": [
    "live",
    "master"
  ],
  "git_repository_url_open_to_public_contributors": "https://github.com/aspnet/EntityFramework.Docs.zh-tw",
  "git_repository_branch_open_to_public_contributors": "live",
  "continue_with_document_error": true,
  "skip_source_output_uploading": false,
  "need_preview_pull_request": true,
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft.zh-TW",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_themes.pdf",
      "url": "https://github.com/Microsoft/templates.docs.msft.pdf",
      "branch": "master"
    },
    {
      "path_to_root": "_repo.en-us",
      "url": "https://github.com/aspnet/EntityFramework.Docs",
      "branch": "live",
      "branch_mapping": {
        "live": "live",
        "master": "master"
      }
    },
    {
      "path_to_root": "efcore-dev",
      "url": "https://github.com/aspnet/EntityFrameworkCore.git",
      "branch": "dev"
    }
  ],
  "need_generate_pdf_url_template": false,
  "branch_target_mapping": { 
    "live-sxs": [ 
      "Publish", 
      "Pdf" 
    ] 
  },
  "Targets": {
    "Pdf": {
      "template_folder": "_themes.pdf"
    }
  }
}
